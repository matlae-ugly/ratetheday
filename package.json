        "popper.js": "^1.12",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.20.1",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-loader": "^15.9.7",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "font-awesome": "^4.7.0",
        "node-sass": "^4.14.1",
        "vue-js-star-rating": "^1.1.1",
        "vue-star-rating": "^1.7.0"
